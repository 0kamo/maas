[Unit]
Description=MAAS Proxy
Requires=network-online.target
After=network-online.target
ConditionPathExists=/usr/share/maas/maas-proxy.conf

[Service]
ExecStartPre=/bin/mkdir -p /run/maas/proxy
ExecStartPre=/bin/sh -ec '\
    . /usr/share/maas/maas-proxy-common.sh; \
    pre_start'
ExecStart=/usr/sbin/squid3 -N -f /usr/share/maas/maas-proxy.conf

[Install]
WantedBy=multi-user.target

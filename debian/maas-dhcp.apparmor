# Work around bug:
# https://bugs.launchpad.net/ubuntu/+source/isc-dhcp/+bug/1186662
capability dac_override,

/run/maas/dhcp/ r,
/run/maas/dhcp/** r,
/run/maas/dhcp/*.pid lrw,
/run/maas/dhcp/*.trace lrw,
/run/maas/dhcp/*.leases* lrw,
/var/lib/maas/dhcp/dhcpd*.leases* lrw,
/var/lib/maas/dhcpd.conf r,
/var/lib/maas/dhcpd6.conf r,
